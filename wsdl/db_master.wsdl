		</wsdl:operation>
	</wsdl:binding>
	
	<wsdl:service name="LimDBMaster">
		<wsdl:port binding="tns:LimDBMasterSOAP" name="LimDBMasterSOAP">
			<soap:address location="@SOAP_LOCATION@/soap/db" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
