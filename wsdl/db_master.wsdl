					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
 			<xsd:element name="DeleteMaster">
 				<xsd:complexType>
 					<xsd:choice>
						<xsd:element ref="lim:Master" maxOccurs="unbounded" />
 					</xsd:choice>
 				</xsd:complexType>
 			</xsd:element>
			<xsd:element name="DeleteMasterResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="lim:Master" maxOccurs="unbounded" minOccurs="0" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="ReadMastersRequest">
		<wsdl:part element="tns:ReadMasters" name="parameters" />
	</wsdl:message>
	
	<wsdl:message name="ReadMastersResponse">
		<wsdl:part element="tns:ReadMastersResponse" name="parameters" />
	</wsdl:message>
	
	<wsdl:message name="ReadMasterRequest">
