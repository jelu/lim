			<xsd:element name="master_host" type="xsd:string" />
			<xsd:element name="master_port" type="xsd:positiveInteger" />
			<xsd:element name="Master">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="lim:master_id" minOccurs="0" />
