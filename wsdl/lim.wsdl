<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="urn:Lim"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	name="Lim"
	targetNamespace="urn:Lim">

	<wsdl:types>
		<xsd:schema targetNamespace="urn:Lim">
			<xsd:element name="GetVersion" />
			<xsd:element name="GetVersionResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="version" type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="GetVersionRequest">
		<wsdl:part element="tns:GetVersion" name="parameters" />
	</wsdl:message>
	
	<wsdl:message name="GetVersionResponse">
		<wsdl:part element="tns:GetVersionResponse" name="parameters" />
	</wsdl:message>
	
	<wsdl:portType name="Lim">
		<wsdl:operation name="GetVersion">
			<wsdl:input message="tns:GetVersionRequest" />
			<wsdl:output message="tns:GetVersionResponse" />
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="LimSOAP" type="tns:Lim">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="GetVersion">
			<soap:operation soapAction="urn:Lim#GetVersion" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	
	<wsdl:service name="Lim">
		<wsdl:port binding="tns:LimSOAP" name="LimSOAP">
			<soap:address location="@SOAP_LOCATION@/soap/lim" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
